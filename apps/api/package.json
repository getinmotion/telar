{
  "name": "getinmotion-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "npm run typeorm migration:run -- -d ./src/config/db/database.config.ts",
    "migration:run:prod": "typeorm migration:run -d ./src/config/db/data-source.prod.js",
    "migration:generate": "npm run typeorm -- -d ./src/config/db/database.config.ts migration:generate ./src/migrations/%npm_config_name%",
    "migration:create": "npm run typeorm -- migration:create ./src/migrations/%npm_config_name%",
    "migration:revert": "npm run typeorm -- -d ./src/config/db/database.config.ts migration:revert",
    "migration:show": "npm run typeorm -- migration:show -d ./src/config/db/database.config.ts",
    "data-migration:all": "ts-node scripts/data-migration/run-all.ts",
    "data-migration:users": "ts-node scripts/data-migration/migrations/01-migrate-users.ts",
    "data-migration:user-profiles": "ts-node scripts/data-migration/migrations/02-migrate-user-profiles.ts",
    "data-migration:user-progress": "ts-node scripts/data-migration/migrations/03-migrate-user-progress.ts",
    "data-migration:agent-tasks": "ts-node scripts/data-migration/migrations/04-migrate-agent-tasks.ts",
    "data-migration:email-verifications": "ts-node scripts/data-migration/migrations/05-migrate-email-verifications.ts",
    "data-migration:master-coordinator-context": "ts-node scripts/data-migration/migrations/06-migrate-master-coordinator-context.ts",
    "data-migration:user-achievements": "ts-node scripts/data-migration/migrations/07-migrate-user-achievements.ts",
    "data-migration:user-master-context": "ts-node scripts/data-migration/migrations/08-migrate-user-master-context.ts",
    "data-migration:user-maturity-scores": "ts-node scripts/data-migration/migrations/09-migrate-user-maturity-scores.ts",
    "data-migration:artisan-shops": "ts-node scripts/data-migration/migrations/10-migrate-artisan-shops.ts",
    "data-migration:product-categories": "ts-node scripts/data-migration/migrations/11-migrate-product-categories.ts",
    "data-migration:products": "ts-node scripts/data-migration/migrations/12-migrate-products.ts",
    "data-migration:user-roles": "ts-node scripts/data-migration/migrations/13-migrate-user-roles.ts"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "11.2.4",
    "@nestjs/typeorm": "^11.0.0",
    "@types/express-session": "^1.18.2",
    "@types/nodemailer": "^7.0.5",
    "@types/passport-google-oauth20": "^2.0.17",
    "bcryptjs": "^3.0.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.3",
    "express-session": "^1.19.0",
    "handlebars": "^4.7.8",
    "nodemailer": "^7.0.12",
    "openai": "^6.16.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.6",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
