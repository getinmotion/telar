# ========================================
# GetInMotion Development Environment
# Docker Compose Configuration
# ========================================
# Copy this file to .env and update with your actual values

# ========================================
# IMAGE CONFIGURATION
# ========================================
# Tag of the GHCR images to pull.
# Defaults to "develop" if not set (matches the CI branch tag).
# To pin a specific commit: IMAGE_TAG=sha-a1b2c3d
IMAGE_TAG=develop

# ========================================
# DATABASE CONFIGURATION (Shared)
# ========================================
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=change_this_to_secure_password
POSTGRES_DB=getinmotion

# ========================================
# API SERVICE (NestJS)
# ========================================
API_PORT=3040
HOST_DB=postgres
PORT_DB=5432
USER_DB=postgres
PASS_DB=change_this_to_secure_password
NAME_DB=getinmotion
PASSWORD_SECRET=change_this_jwt_secret_at_least_32_characters_long_recommended_64
OPENAI_API_KEY=sk-your-openai-api-key-here
NODE_ENV=development
ENVIRONMENT_PROJECT=dev

# ========================================
# GOOGLE OAUTH
# ========================================
# Get from: https://console.cloud.google.com → APIs & Services → Credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_CALLBACK_URL=http://YOUR_SERVER_IP:3040/telar/server/auth/google/callback

# ========================================
# AGENTS SERVICE (Python/FastAPI)
# ========================================
AGENTS_PORT=8000
# OpenAI Configuration
OPENAI_MODEL=gpt-4o
EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_DIMENSIONS=1536

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
SUPABASE_DB_URL=postgresql://postgres:change_this_to_secure_password@postgres:5432/getinmotion

# API Security
API_SECRET_KEY=your_secret_key_here_generate_with_openssl_rand_hex_32

# Server Configuration
PORT=8000
HOST=0.0.0.0
LOG_LEVEL=info

# LangSmith (Optional - leave empty to disable)
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=artisan-agents
LANGSMITH_TRACING=false

# Tavily Web Search
TAVILY_API_KEY=your_tavily_api_key_here

# Agent Configuration
MAX_RETRIES=3
REQUEST_TIMEOUT=60
RAG_TOP_K=5
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
MAX_BATCH_SIZE=100

# Memory Configuration
MEMORY_RETRIEVAL_LIMIT=10
MEMORY_IMPORTANCE_THRESHOLD=0.5
PROFILE_UPDATE_INTERVAL=5
CONVERSATION_SUMMARY_INTERVAL=10

# ========================================
# ARTISANS-WEB (Vite/React)
# ========================================
ARTISANS_WEB_PORT=3000
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=your_supabase_anon_or_publishable_key_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_BACKEND_URL=http://localhost:3040
VITE_SEMANTIC_SEARCH_URL=http://localhost:8000/search
VITE_SEMANTIC_SEARCH_API_KEY=your_semantic_search_api_key
VITE_GOOGLE_PLACES_API_KEY=your_google_places_api_key

# ========================================
# MARKETPLACE-WEB (Vite/React)
# ========================================
MARKETPLACE_WEB_PORT=3001
MARKETPLACE_VITE_SUPABASE_URL=https://your-project-id.supabase.co
MARKETPLACE_VITE_SUPABASE_PUBLISHABLE_KEY=your_supabase_anon_or_publishable_key_here
MARKETPLACE_VITE_SEMANTIC_SEARCH_API_URL=http://localhost:8000/search
MARKETPLACE_VITE_SEMANTIC_SEARCH_API_KEY=your_semantic_search_api_key
